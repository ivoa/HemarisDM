            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="EnumValueUpdate">
      <xsd:annotation>
         <xsd:documentation>A simple enum value update.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:EnumValueUpdate</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="execbr:AbstractUpdate">
            <xsd:sequence>
               <xsd:element name="value" type="xsd:string" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>The enum value to use.</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:EnumValueUpdate.value</vodml-ref>
