   <xsd:complexType name="DockerPlatformSpec">
      <xsd:annotation>
         <xsd:documentation>Details of the platform the container image is built for. Debatable whether we need to have this here. The information will be in the container, and the platform will probably need to check it anyway to make sure. So why have the duplicate information ?</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:DockerPlatformSpec</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="architecture"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The CPU architecture the image is built for. The default is `amd64`.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:DockerPlatformSpec.architecture</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="os" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The operating system the image is built for. The default is `linux`.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:DockerPlatformSpec.os</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="NameValueMap">
      <xsd:annotation>
         <xsd:documentation>A map of name-&gt;value properties. See https://swagger.io/docs/specification/data-models/dictionaries/</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:NameValueMap</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="key" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The key</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:NameValueMap.key</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="value" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The value</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:NameValueMap.value</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DockerInternalPort">
      <xsd:annotation>
         <xsd:documentation>Details of an internal network port on a container.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:DockerInternalPort</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="port" type="xsd:int" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The port number on the container to publish. This should be defined as part of the description of the executable.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:DockerInternalPort.port</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DockerExternalPort">
      <xsd:annotation>
         <xsd:documentation>Details of an external network port accessible from outside.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:DockerExternalPort</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="port" type="xsd:int" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The port number of the external interface. The service will populate this value when it sets up the execution.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:DockerExternalPort.port</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element minOccurs="1" name="addresses">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="addresse"
                               type="xsd:string"
                               minOccurs="1"
                               maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation>A list of public hostnames or IPv4 or IPv6 addresses for the external interface. The service will populate this list when it sets up the execution.</xsd:documentation>
                        <xsd:appinfo>
                           <vodml-ref>execbroker:DockerExternalPort.addresses</vodml-ref>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="LifecycleStartDurationInstant">
      <xsd:annotation>
         <xsd:documentation>A lifecycle instant and duration.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:LifecycleStartDurationInstant</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="start" type="dateTime" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>execbroker:LifecycleStartDurationInstant.start</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="duration" type="duration" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>execbroker:LifecycleStartDurationInstant.duration</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="LifecycleStartDurationInterval">
      <xsd:annotation>
         <xsd:documentation>A lifecycle interval and duration.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:LifecycleStartDurationInterval</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
