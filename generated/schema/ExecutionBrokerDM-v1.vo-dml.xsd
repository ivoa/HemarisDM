   <xsd:complexType name="SimpleSessionConnector">
      <xsd:annotation>
         <xsd:documentation>A connector to interact with the session.
               </xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:SimpleSessionConnector</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="kind" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A URI to identify the type of connector.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:SimpleSessionConnector.kind</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="status" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>A flag to indicate the staus of the access method.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:SimpleSessionConnector.status</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="protocol" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The access protocol, provided by the server when an offer is made. Typically one of [TCP, HTTP, HTTPS].</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:SimpleSessionConnector.protocol</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="location" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The access connector URL.</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>execbroker:SimpleSessionConnector.location</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SimpleExecutionSession">
      <xsd:annotation>
         <xsd:documentation>A simple execution session.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:SimpleExecutionSession</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="execbr:AbstractExecutionSession">
            <xsd:sequence>
               <xsd:element name="phase"
                            type="execbr:SimpleExecutionSessionPhase"
                            minOccurs="1"
                            maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation/>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:SimpleExecutionSession.phase</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="expires" type="xsd:dateTime" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>The date and time when this offer expires. Only valid while the state is OFFERED</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:SimpleExecutionSession.expires</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="components"
                            type="execbr:SimpleExecutionComponents"
                            minOccurs="1"
                            maxOccurs="1"/>
               <xsd:element name="connectors">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="simpleSessionConnector"
                                     type="execbr:SimpleSessionConnector"
                                     minOccurs="1"
                                     maxOccurs="unbounded"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ScheduledExecutionSchedule">
      <xsd:annotation>
         <xsd:documentation>An Execution Session schedule.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:ScheduledExecutionSchedule</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="preparing"
                      type="execbr:ScheduleStartDurationInstant"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>execbroker:ScheduledExecutionSchedule.preparing</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="available"
                      type="execbr:ScheduleStartDurationInterval"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>execbroker:ScheduledExecutionSchedule.available</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="releasing"
                      type="execbr:ScheduleStartDurationInstant"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>execbroker:ScheduledExecutionSchedule.releasing</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ScheduledExecutionSession">
      <xsd:annotation>
         <xsd:documentation>A scheduled execution session.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:ScheduledExecutionSession</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="execbr:SimpleExecutionSession">
            <xsd:sequence>
               <xsd:element name="schedule"
                            type="execbr:ScheduledExecutionSchedule"
                            minOccurs="1"
                            maxOccurs="1"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="OfferSetResponse">
      <xsd:annotation>
         <xsd:documentation>A set of executions offered in response to a request, including a uuid, href, and an expiry date for the set.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:OfferSetResponse</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="execbr:AbstractComponent">
            <xsd:sequence>
               <xsd:element name="result" type="xsd:string" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>A flag to indicate whether the request can be handled by this service. If service is able to handle the request, then the `result` will be `YES` and the `offers` block should contain one or more offers. If service is not able to handle the request, the `result` will be `NO` and the `messages` block should contain one or more reasons explaining why.</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:OfferSetResponse.result</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="description"
                            type="xsd:string"
                            minOccurs="1"
                            maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>A human readable description.</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:OfferSetResponse.description</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="offers">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="abstractExecutionSession"
                                     type="execbr:AbstractExecutionSession"
                                     minOccurs="1"
                                     maxOccurs="unbounded"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AbstractUpdate">
