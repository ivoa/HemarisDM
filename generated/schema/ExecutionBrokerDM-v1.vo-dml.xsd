   <xsd:complexType name="IntegerDeltaUpdate">
      <xsd:annotation>
         <xsd:documentation>A simple integer delta update. This increments or decrements the target value by the specified amount.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>execbroker:IntegerDeltaUpdate</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="execbr:AbstractUpdate">
            <xsd:sequence>
               <xsd:element name="delta" type="xsd:int" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>The increment or decrement to apply.</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:IntegerDeltaUpdate.delta</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="units" type="xsd:string" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>The units to use for the change.</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>execbroker:IntegerDeltaUpdate.units</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
