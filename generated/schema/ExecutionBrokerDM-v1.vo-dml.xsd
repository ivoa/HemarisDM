                        <vodml-ref>execbroker:IntegerValueUpdate.value</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="units" type="xsd:string" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>The units to use for the value.</xsd:documentation>
